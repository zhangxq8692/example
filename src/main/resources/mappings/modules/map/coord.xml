<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhang.modules.coord.dao.CoordDao">
    <sql id="column">
        id,
        p_lng,
        p_lat,
        car_id,
        create_date,
        update_date,
        update_flag
    </sql>
    <select id="findById" resultType="coord" useCache="true">
        SELECT
          <include refid="column"/>
        FROM
          sys_coord
        WHERE
          id=#{id}
    </select>
    <select id="findAll" resultType="coord">
        SELECT
          <include refid="column"/>
        FROM
          sys_coord
    </select>
    <update id="update">
        UPDATE sys_coord SET
        p_lng=#{pLng},
        p_lat=#{pLat},
        car_id=#{carId},
        create_date=#{createDate},
        update_date=#{updateDate},
        update_flag=#{updateFlag}
        <where>
            id=#{id}
        </where>
    </update>
    <delete id="delete">
        DELETE FROM sys_coord WHERE id=#{id}
    </delete>
    <insert id="insert">
        insert into sys_coord
        (
        <include refid="column"/>
        )
        values
        (
        #{id},
        #{pLng},
        #{pLat},
        #{carId},
        #{createDate},
        #{updateDate},
        #{updateFlag}
        )
    </insert>
</mapper>